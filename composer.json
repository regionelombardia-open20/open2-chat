{
    "name": "lispa/amos-chat",
    "description": "Sistema AMOS per la chat",
    "keywords": ["amos", "yii2", "chat"],
    "homepage": "http://git.lispa.it/lispa/amos-chat",
    "type": "component",
    "proscription": "BSD-3-Clause",
    "support": {
        "issues": "http://git.lispa.it/lispa/amos-chat/issues",
        "forum": "http://www.example.com/forum/",
        "wiki": "http://git.lispa.it/lispa/amos-chat/wikis/home",
        "source": "http://git.lispa.it/lispa/amos-chat/tree/master"
    },
    "require": {
        "php": ">=5.4.0",
        "lispa/amos-core" : "*",
        "lispa/amos-admin" : "*",
        "lispa/amos-email-manager" : "*",
        "kop/yii2-scroll-pager": "~2.2"
    },
    "config": {
        "process-timeout": 1800,
        "secure-http": false
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "autoload": {
        "psr-4": {
            "lispa\\amos\\chat\\": "src"
        }
    },
    "require-dev": {
        "yiisoft/yii2-faker": "*"
    }
}